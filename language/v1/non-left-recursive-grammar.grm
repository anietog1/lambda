{ # lambda language grammar definition
 { # terminals
  ID = '[a-z]', # single letter identifiers
  '\', # 'lambda' symbol
  '.', # separator
  '(', ')' # separators
 },
 { # non-terminals
  exp -> ID # variable name
       | '(' exp ')' # precedence
       | '\' ID '.' exp # definition
       # expanding | exp exp # application
       | ID exp
       | '(' exp ')' exp
       | '\' ID '.' exp exp
 },
 exp # starting symbol
}
